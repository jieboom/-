.channel-container{
    
    display: flex;
    flex-wrap: wrap;
   
}
.channel-item{
    position: relative;
    width:  375rpx;
    height: 375rpx;

}
.channel-item .channel-img{
    width: 100%;
    height: 100%;
    opacity: 0.9;
}
.channel-text{
    display: block;
    
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    line-height: 375rpx;
    text-align: center;
   color: #fff;
   font-weight: bold;
   font-size: 30rpx;
}